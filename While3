package While;
import java.util.Scanner;
public class While3 {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        System.out.print("N  ");
        int n = scanner.nextInt();

        System.out.print("K  ");
        int k = scanner.nextInt();

        int q= 0;
        int r = n;

        while (r >= k) {
            r-= k;
            q++;
        }


        System.out.println("Butun qismi: " + q);
        System.out.println("Qoldiq: " + r);


    }
}
